{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3UploadBucketAccess",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject", "s3:DeleteObject"],
      "Resource": "arn:aws:s3:::stenoai-*-uploads/*"
    },
    {
      "Sid": "S3ExportBucketAccess",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject", "s3:DeleteObject"],
      "Resource": "arn:aws:s3:::stenoai-*-exports/*"
    }
  ]
}
