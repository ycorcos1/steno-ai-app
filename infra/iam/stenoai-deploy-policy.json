{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "StenoAIFullAccess",
      "Effect": "Allow",
      "Action": [
        "s3:*",
        "cloudfront:*",
        "lambda:*",
        "apigateway:*",
        "bedrock:*",
        "ec2:*",
        "rds:*",
        "dynamodb:*",
        "secretsmanager:*",
        "logs:*",
        "cloudwatch:*",
        "iam:GetRole",
        "iam:GetRolePolicy",
        "iam:ListRolePolicies",
        "iam:ListAttachedRolePolicies",
        "iam:CreateRole",
        "iam:AttachRolePolicy",
        "iam:PutRolePolicy",
        "iam:UpdateAssumeRolePolicy",
        "iam:PassRole",
        "iam:TagRole",
        "iam:GetPolicy",
        "iam:GetPolicyVersion",
        "iam:ListPolicyVersions",
        "iam:CreatePolicy",
        "iam:CreatePolicyVersion",
        "iam:AttachUserPolicy",
        "iam:DetachUserPolicy",
        "iam:ListAttachedUserPolicies",
        "iam:TagPolicy"
      ],
      "Resource": "*"
    }
  ]
}
